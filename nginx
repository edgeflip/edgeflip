uwsgi_cache_path /var/www/edgeflip/CACHE keys_zone=CACHE:10m;

server {
    listen 80;
    server_name $CLIENT_SUBDOMAIN;

    location /static {
        alias /var/www/edgeflip/edgeflip/web/static;
    }

    location / {
        include uwsgi_params;
        uwsgi_pass unix:/run/uwsgi/app/edgeflip/socket;
    }
}
